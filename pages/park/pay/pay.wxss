.status {
  width: 100%;
  height: 80rpx;
  color: gray;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
  font-size: 20rpx;
  border-bottom: 5px solid gainsboro;
}
.num {
  color: lightgreen;
  margin-left: 10rpx;
  font-size: 30rpx;
}
.button {
  height: 100rpx;
  width: 100%;
  /* background-color: black; */
  color: white;
  bottom: 50rpx;
  position: absolute;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}
.button button {
  width: 90%;
  background-color: black;
  color: white;
}

.cards {
   /* display: ;  */
  width: 90%;
  height: 100rpx;
   display: flex; 
  flex-direction: row;
  justify-content: center;
  align-items: center;
  text-align: center;
  border-bottom: 1px solid gainsboro;
  margin: auto auto;
  position: relative;
}
.card {
  width: 25%;
  height: 50%;
  border-right: 1px solid gainsboro;
}
.add {
  width: 25%;
  height: 100%;
  color: #2682D7;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  font-size: 30rpx;
}
.coupon {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  font-size: 30rpx;
  font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
  width: 90%;
  margin: auto auto;
  height: 80rpx;
  border-bottom: 1px solid gainsboro;
}
.coupon-type {
  color: gray;
}
.ticket {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  font-size: 30rpx;
  font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
  width: 90%;
  margin: auto auto;
  height: 80rpx;
  border-bottom: 1px solid gainsboro;
}
.ticket-type {
  color: gray;
}
.park {
  width: 90%;
  height: 200rpx;
  background-color: gainsboro;
  margin: auto auto;
  border: 1px solid gainsboro;
  border-radius: 5px;
  text-align: center;
  margin-top: 80rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}
.cardinput {
  width: 90%;
  height: 100rpx;
  background-color: white;
  margin:auto auto;
  position: relative;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}
.subpicker{
  width: 500rpx;
  height: 100rpx;
  font-size: 30rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.cardinput input {
  width: 70%;
  height: 90%;
  /* border: 10rpx solid lightgray; */
  border-bottom: 1px solid gray;
  font-size: 30rpx;
  font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
  text-align: center;
  /* border-radius: 5px; */
}
.cardinputcancel  {
  font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
  font-size: 30rpx;
  /* border: 1px solid lightblue; */
  border-right: 1px solid gray;
  border-left: 1px solid gray;
  color: red;
  height: 50%;
  width: 15%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.cardinputconfirm {
  font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
  font-size: 30rpx;
  /* border: 1px solid lightblue; */
  color: green;
  height: 70%;
  width: 15%;
  display: flex;
  align-items: center;
  justify-content: center;
}

/* pages/pay/pay.wxss */
page {
  min-height: 100%;
  background-color: #f6f6f6;
  font-family: "微软雅黑";
  font-size: 30rpx;
  color: #353535;
  width: 100%;
}

.section {
  background-color: #fff;
}

.item {
  display: flex;
  justify-content: space-between;
  margin-left: 30rpx;
  padding: 5rpx 30rpx 5rpx 0;
  line-height: 90rpx;
  border-bottom: 1rpx solid #e0e0e0;
}

.addre1 {
  margin-left: 30rpx;
  display: flex;
  justify-content: space-between;
  padding-top: 30rpx;
  line-height: 100rpx;
}

.addre2 {
  line-height: 60rpx;
  font-size: 27rpx;
}

.addre2 .left {
  display: flex;
  flex-direction: column;
  width: 90%;
}

.addre2 .left .area {
  float: right;
}

.addre2 .left_bottom {
  padding-left: 40rpx;
  color: #888;
}

.left_top image {
  padding-top: 10rpx;
  margin-right: 10rpx;
}

.more {
  width: 25rpx;
  float: right;
  margin-right: 20rpx;
  padding-top: 40rpx;
}

image {
  width: 30rpx;
}

.line image {
  width: 100%;
  height: 20rpx;
}

.red {
  color: #e64340;
}

.gray {
  color: #939393;
}

.right {
  display: flex;
  width: 400rpx;
}
.paytitle{
  width: 250rpx;
  height: 80rpx;
  margin: 60rpx auto;
  margin-bottom: 30rpx;
  text-align: center;
}